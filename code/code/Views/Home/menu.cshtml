@model IEnumerable<code.Models.Page>
<span class="menu-trigger">MENU</span>
<div class="nav nav-expanded">
    <div class="nav-inner">
        <ul class="clearfix">
            @foreach(var item in Model)
            {
                string alias = item.Alias + ".html";
                <li><a href="~/@alias">@item.Title</a></li>
            }
        </ul>
    </div>
</div>